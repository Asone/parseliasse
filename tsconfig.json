{
    "compilerOptions": {
      "target": "es6",
      "lib" : ["es5", "es6", "dom","esnext.asynciterable"],
      "module": "commonjs",
      "declaration": true,
      "outDir": "./dist",
      "strict": true,
      "resolveJsonModule": true
    },
    "include": [
      "./index.ts",
      "src/**/*.ts"
